{
  "name": "merge-guard-bot",
  "version": "1.0.0",
  "description": "prevent unintentional github pull request merge",
  "author": "Sandy Akbar Dewangga <sandy.a.dewangga@gdplabs.id>",
  "license": "ISC",
  "repository": "https://github.com//merge-guard-bot.git",
  "homepage": "https://github.com/apps/merge-guard-bot",
  "bugs": "https://github.com//merge-guard-bot/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app",
    "merge-guard-bot"
  ],
  "scripts": {
    "dev": "nodemon",
    "start": "probot run ./index.js",
    "lint": "standard --fix",
    "test": "jest && standard",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "probot": "^12.2.8",
    "probot-metadata": "^1.0.0"
  },
  "devDependencies": {
    "jest": "^24.0.0",
    "nock": "^10.0.0",
    "nodemon": "^1.17.2",
    "smee-client": "^1.0.2",
    "standard": "^12.0.1"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
